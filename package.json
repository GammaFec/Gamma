{
    "name": "gamma-project",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "firebase": "8.0.2",
        "i18next": "^19.9.0",
        "i18next-browser-languagedetector": "^6.0.1",
        "moment": "^2.29.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
